:logfile:     <%= @log_file    || '/var/log/labpages/sidekiq.log' %>
:pidfile:     <%= @pid_file    || '/var/run/labpages/sidekiq.pid' %>
:verbose:     <%= @verbose     || true                            %>
:concurrency: <%= @concurrency || 5                               %>
:timeout:     <%= @timeout     || 10                              %>
:queues:
  - [<%= @queue || 10 %>, <%= @concurrency || 5 %>]